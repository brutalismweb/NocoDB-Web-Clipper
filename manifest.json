{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.1",
  "description": "__MSG_extensionDescription__",
  "default_locale": "ru",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://192.168.0.138:8081/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extensionName__"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}